{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/convert",
      "methods": ["POST"],
      "dest": "api/app.py"
    },
    {
      "src": "/(.*)",
